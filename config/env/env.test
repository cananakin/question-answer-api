# Server variables

PORT=5000
NODE_ENV=development

# MongoDB Connection
MONGO_URI = <YOUR_MONGO_URI>

# Json Web Token
JWT_SECRET_KEY=<YOUR_SECRET_KEY>
JWT_EXPIRE=30m 

# Cookie
JWT_COOKIE_EXPIRE=30

# Reset Password
RESET_PASSWORD_EXPIRE=3600000

# Nodemailer

SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=<YOUR_GMAIL>
SMTP_PASS=<YOUR_GMAIL_PASSWORD>